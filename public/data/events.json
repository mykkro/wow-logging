{
	"$type": "wow-logging-events-list",
	"classes": [
		{
			"name": "event",
			"className": "AbstractEvent",
			"extends": null,
			"description": "Deklarace pro vsechny typy udalosti.",
			"fields": {
				"type": {
					"title": "type",
					"description": "Rozlisuje typ udalosti (app, user, admin).",
					"type": "string"
				},
				"timestamp": {
					"title": "timestamp",
					"description": "Casova znacka udalosti.",
					"type": "datetime"
				},
				"id": {
					"title": "id",
					"description": "Identifikace udalosti v ramci aplikace.",
					"type": "integer"
				}
			}
		},
		{
			"name": "app.event",
			"className": "AppEvent",
			"extends": "AbstractEvent",
			"description": "Udalost aplikace (app. prefix pro udalosti aplikace)."
		},
		{
			"name": "app.startup",
			"uses": "AppEvent",
			"description": "Udalost spusteni aplikace."
		},
		{
			"name": "app.shutdown",
			"uses": "AppEvent",
			"description": "Udalost zastaveni aplikace."
		},
		{
			"name": "admin.event",
			"className": "AdminEvent",
			"extends": "AbstractEvent",
			"description": "Udalost administrator (admin. prefix pro administrator udalosti)."
		},
		{
			"name": "admin.appimport",
			"className": "AdminImportAppEvent",
			"extends": "AdminEvent",
			"description": "Udalost importu v aplikaci prostrednictvim administratora."
		},
		{
			"name": "admin.usercreate",
			"className": "AdminUserCreatedEvent",
			"extends": "AdminEvent",
			"description": "Udalost vytvoreni uzivatele aplikace administratorem."
		},
		{
			"name": "admin.userchange",
			"className": "AdminUserChangeEvent",
			"extends": "AdminEvent",
			"description": "Udalost zmeny uzivatele aplikace administratorem."
		},
		{
			"name": "user.event",
			"className": "UserEvent",
			"extends": "AbstractEvent",
			"description": "Udalost uzivatel (user. prefix pro uzivatel udalosti)."
		},
		{
			"name": "user.login",
			"uses": "UserEvent",
			"description": "Udalost prihlaseni uzivatele aplikace."
		},
		{
			"name": "user.logout",
			"uses": "UserEvent",
			"description": "Udalost odhlaseni uzivatele aplikace."
		},
		{
			"name": "user.page",
			"className": "UserPageEvent",
			"extends": "UserEvent",
			"description": "Udalost stranky uzivatele.",
			"fields": {
				"uri": {
					"title": "uri",
					"description": "Identifikator zdroje stranky.",
					"type": "uri"
				}
			}
		},
		{
			"name": "video.event",
			"className": "VideoEvent",
			"extends": "AbstractEvent",
			"description": "Udalost videa (video. prefix pro video udalosti)."
		},
		{
			"name": "video.textsearch",
			"className": "VideoSearchEvent",
			"extends": "VideoEvent",
			"description": "",
			"fields": {
				"query": {
					"title": "query",
					"description": "Vyraz pouzity k vyhledani videi s odpovidajicim tematem.",
					"type": "string"
				},
				"page": {
					"title": "page",
					"description": "Cislo stranky s videama.",
					"type": "integer"
				},
				"recordsPerPage": {
					"title": "recordsPerPage",
					"description": "Pocet videi na strance.",
					"type": "integer"
				}
			}
		},
		{
			"name": "video.play",
			"uses": "VideoEvent",
			"description": "Udalost prehrani videa."
		},
		{
			"name": "video.pause",
			"uses": "VideoEvent",
			"description": "Udalost pozastaveni prehravani videa."
		},
		{
			"name": "video.stop",
			"uses": "VideoEvent",
			"description": "Udalost zastaveni prehravani videa."
		},
		{
			"name": "video.favorite",
			"uses": "VideoEvent",
			"description": "Udalost prirazeni videa k oblibenym."
		},
		{
			"name": "video.unfavorite",
			"uses": "VideoEvent",
			"description": "Udalost odstraneni videa z obligenych."
		},
		{
			"name": "game.event",
			"className": "GameEvent",
			"extends": "AbstractEvent",
			"description": "Udalost hry (game. prefix pro udalosti hry)."
		},
		{
			"name": "game.loaded",
			"uses": "GameEvent",
			"description": "Udalost nacteni hry."
		},
		{
			"name": "game.unloaded",
			"uses": "GameEvent",
			"description": "Udalost uvolneni(odstraneni) hry."
		},
		{
			"name": "game.play",
			"uses": "GameEvent",
			"description": "Udalost spusteni hry."
		},
		{
			"name": "game.pause",
			"uses": "GameEvent",
			"description": "Udalost pozastaveni hry."
		},
		{
			"name": "game.quit",
			"uses": "GameEvent",
			"description": "Udalost ukonceni hry."
		},
		{
			"name": "game.setting",
			"uses": "GameEvent",
			"description": "Udalost nastaveni hry uzivatelem."
		}



	]
}
